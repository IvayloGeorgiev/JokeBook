.container
    .jumbotron
        h2 Jokes with tag: {{tag}}


        table.table.table-striped
            thead
                tr
                    td Joke Title
                    td Joke Author
                    td Date Created
                    td Type
                    td Likes
            tbody
                tr(ng-repeat="joke in jokes")
                    td {{ joke.title }}
                    td
                        a(href="#/user-details/{{comment.user._id}}") {{ joke.user.username }}
                    td {{ joke.date | date: 'medium'}}
                    td
                        a(href="#/jokes-by-tag/{{joke.tags[0]}}") {{ joke.tags[0]}}
                    td {{ joke.likes }}
                    td
                        a.btn.btn-primary.btn-xs(href="#/joke/{{joke._id}}") View